{
  frankenphp {
    admin :2020
    php_ini {
        memory_limit 2560M
        max_execution_time 15
    }
  }
}
api.microstorm.local {
    rewrite * /worker.php?request={path}&{query}.replace('?','&')
    php_server {
	root /Application/Public
	worker worker.php 2
        try_files {path} index.php
    }
}