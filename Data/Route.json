{
  "Route": [
    {
      "name": "boot",
      "path": "boot",
      "priority": 1000,
      "controller": "Microstorm:Cli:Boot.run",
      "request": {
        "language": "en"
      },
      "method": [
        "CLI"
      ]
    },
    {
      "name": "*",
      "controller": "Microstorm:Controller:Index:main",
      "priority": 2001,
      "method": [
        "GET",
        "POST"
      ]
    },
    {
      "name": "index",
      "controller": "Microstorm:Controller:Index:main",
      "path": "\/",
      "priority": 2002,
      "method": [
        "GET",
        "POST"
      ],
      "request": {
        "page" : "home"
      }
    }
  ]
}
